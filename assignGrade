function assignGrade(){
  var i = Math.floor((Math.random() * 100) + 1)
    if(i>= 90){
  console.log(i + " You got a A");
    }
  else if (i>= 80){
  console.log(i + " You got a B");
  }
  else if (i>= 70){
  console.log(i + " You got a C");
  }
  else if (i>= 60){
  console.log(i + " You got a D");
  }
  else
  {
  console.log(i + " You got a F");
  }
  }
  assignGrade();
